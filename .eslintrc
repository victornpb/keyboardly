{
  "name": "foo-bar",
  "nameFull": "FooBar",
  "version": "0.0.0",
  "description": "A foo-bar library",
  "license": "MIT",
  "engines": {
    "node": ">=6"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/username/foo-bar.git"
  },
  "bugs": {
    "url": "https://github.com/username/foo-bar/issues"
  },
  "homepage": "https://github.com/username/foo-bar",
  "author": "username",
  "main": "dist/foo-bar.cjs.js",
  "module": "dist/foo-bar.esm.js",
  "unpkg": "dist/foo-bar.js",
  "globalVar": "FooBar",
  "scripts": {
    "start": "rollup -c -w",
    "dev": "rollup -c -w",
    "build": "rollup -c",
    "lint": "eslint src/**/*.js",
    "lint:fix": "prettier-eslint src/**/*.js --write",
    "prebuild": "rimraf dist",
    "prepublishOnly": "npm run build && git add * && git commit -m \"Updating build\"",
    "release": "np patch --any-branch"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "foo",
    "bar"
  ],
  "sideEffects": false,
  "dependencies": {
    "penner": "^0.1.3"
  },
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/plugin-proposal-private-methods": "^7.16.11",
    "@babel/plugin-syntax-optional-chaining": "^7.8.3",
    "@babel/plugin-transform-object-assign": "^7.16.7",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.16.11",
    "asciitable.js": "^1.0.3",
    "eslint": "^8.13.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.7",
    "np": "^7.6.1",
    "postcss": "^8.4.12",
    "rimraf": "^3.0.2",
    "rollup": "^2.70.1",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-banner2": "^1.2.2",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-terser": "^7.0.2",
    "tiny-dedent": "^1.0.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.js": [
      "npm run lint:fix",
      "git add"
    ]
  }
}
